{
  "name": "gogs",
  "trademark": "Gogs",
  "installpath": "/data/wwwroot/gogs",
  "compose_file": "docker-compose.yml",
  "fork_url": "https://github.com/gogs/gogs",
  "env_file": ".env",
  "requirements":{
    "cpu_arch": "Linux x86-64, ARM 32/64, IBM POWER8, x86/i686",
    "cpu": "1",
    "memory": "2",
    "disk": "20",
    "swap": "2",
    "url": "https://github.com/gogs/gogs"
  },

  "services":[
    {
      "name": "gogs",
      "port": "9001",
      "description_cn": "浏览器访问 Gogs",
      "description_en": "HTTP to access Gogs",
      "required": "Y"
    },

    {
      "name": "gogs",
      "port": "9002",
      "description_cn": "SSH 访问 Gogs",
      "description_en": "SSH to access Gogs",
      "required": "Y"
    }
  ]

}